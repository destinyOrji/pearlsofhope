{
  "functions": {
    "**/*.php": {
      "runtime": "vercel-php@0.8.0"
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.php"
    },
    {
      "src": "/about",
      "dest": "/public/about.php"
    },
    {
      "src": "/activities",
      "dest": "/public/activities.php"
    },
    {
      "src": "/activity-detail",
      "dest": "/public/activity-detail.php"
    },
    {
      "src": "/contact",
      "dest": "/public/contact.php"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/admin/$1"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "/uploads/$1"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}